# ---------------------------------------------------------------------------
!if !$d(BCB)
BCB = $(MAKEDIR)\..
!endif

# ---------------------------------------------------------------------------
# IDE SECTION
# ---------------------------------------------------------------------------
# The following section of the project makefile is managed by the BCB IDE.
# It is recommended to use the IDE to change any of the values in this
# section.
# ---------------------------------------------------------------------------

VERSION = BCB.04.04
# ---------------------------------------------------------------------------
PROJECT = borland54.exe
OBJFILES = borland54.obj ..\ex1.obj
RESFILES =
RESDEPEN = $(RESFILES)
LIBFILES = ..\..\..\lib\win32\libmatpb54.lib ..\..\..\lib\win32\_libmat.lib \
  ..\..\..\lib\win32\_libmatlb.lib ..\..\..\lib\win32\_libmmfile.lib \
  ..\..\..\lib\win32\_libmx.lib
LIBRARIES =
SPARELIBS = 
PACKAGES = vclx40.bpi vcl40.bpi vcldb40.bpi vcldbx40.bpi bcbsmp40.bpi dclocx40.bpi \
  qrpt40.bpi
DEFFILE =
# ---------------------------------------------------------------------------
PATHCPP = .;..
PATHASM = .;
PATHPAS = .;
PATHRC = .;
DEBUGLIBPATH = $(BCB)\lib\debug
RELEASELIBPATH = $(BCB)\lib\release
SYSDEFINES = NO_STRICT;_NO_VCL
USERDEFINES = BORLAND;IBMPC;MSWIND
# ---------------------------------------------------------------------------
CFLAG1 = -I..\;..\..\cppmath;..;..\..\..\include\cpp;..\..\..\include;$(BCB)\include -Od \
  -w -Tkh30000 -r- -a8 -k -y -v -vi- -D$(SYSDEFINES);$(USERDEFINES) -c -tWC -tWM
CFLAG2 =
CFLAG3 =
PFLAGS = -U..\;..\..\cppmath;..;$(BCB)\lib;..\..\..\lib\win32;$(DEBUGLIBPATH) \
  -I..\;..\..\cppmath;..;..\..\..\include\cpp;..\..\..\include;$(BCB)\include \
  -DBORLAND;IBMPC;MSWIND -$YD -$W -$O- -v -M -JPHNE
RFLAGS = -i..\;..\..\cppmath;..;..\..\..\include\cpp;..\..\..\include;$(BCB)\include \
  -DBORLAND;IBMPC;MSWIND
AFLAGS = /i..\..\cppmath /i.. /i..\..\..\include\cpp /i..\..\..\include /i$(BCB)\include \
  /dBORLAND /dIBMPC /dMSWIND /mx /w2 /zd
LFLAGS = -L..\;..\..\cppmath;..;$(BCB)\lib;..\..\..\lib\win32;$(DEBUGLIBPATH) -ap -Tpe \
  -x -Gn -v
IFLAGS =
# ---------------------------------------------------------------------------
ALLOBJ = c0x32.obj $(OBJFILES)
ALLRES = $(RESFILES)
ALLLIB = $(LIBFILES)  import32.lib cw32mt.lib
# ---------------------------------------------------------------------------
